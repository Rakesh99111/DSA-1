class GFG
{
    ArrayList<Long> find(long arr[], int n, int x)
    {
        long first=-1,last=-1;
        // code here
         for(int i=0;i<n;i++){
        if(arr[i]==x){
            first =i;
        }
    }
    for(int i=n-1;i>=0;i--){
        if(arr[i]==x){
            last =i;
        }
    }
   
    
    ArrayList<Long> list = new ArrayList<Long>();
    
    list.add(last);
    list.add(first);
    
    return list;
    }
}
